{
  "name": "GitStub",
  "description": "Recommends GitHub projects",
  "version": "1.0.2",
  "icons" : {
    "64" : "logo64.png",
    "128" : "logo128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "tabs",
    "storage",
    "*://*.github.com/*"
  ],
  "content_scripts": [
    {
      "matches" : [
        "*://www.github.com/*",
        "*://github.com/*" 
      ],
      "css" : ["style.css"],
      "js" : [ "data_col.js", "recommend.js"]
    }
  ],
  "web_accessible_resources" : [
    "github_APITOKEN"
  ],
  "manifest_version": 2
}
